    "version": "0.2.0",
    "configurations": [    
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "outFiles": ["${workspaceFolder}/dist/*.js"],
            "sourceMapPathOverrides": {
                "webpack://mvc/*": "${workspaceRoot}/*"
            },
            "program": "${workspaceFolder}/../../.nvm/versions/node/v20.11.1/lib/node_modules/npm/node_modules/@npmcli/run-script/lib/signal-manager.js",
            "trace": true,
        }
    ]
}